<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Detalles">
    <RelativeLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        xmlns:android="http://schemas.android.com/apk/res/android" >


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Recibo:"
            android:textSize="16sp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:id="@+id/informacion"
            android:textColor="#000000"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tiempo"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/sbTotal"
            android:layout_below="@+id/teimpo"
            android:textColor="#000000"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="01h:30m:10s"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/t"
            android:layout_alignBaseline="@+id/sbTotal"
            android:layout_alignParentRight="true"
            android:textColor="#000000"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Establecimiento"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/lugar"
            android:layout_below="@+id/tiempoFrac"
            android:textColor="#000000"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Plazas Outlet Lerma"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/lu"
            android:layout_alignBaseline="@+id/lugar"
            android:layout_alignParentRight="true"
            android:textColor="#000000"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Fecha"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/teimpo"
            android:layout_below="@+id/lugar"
            android:textColor="#000000"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="25/10/17"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/fe"
            android:layout_alignBaseline="@+id/teimpo"
            android:layout_alignParentRight="true"
            android:textColor="#000000"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tarifa base(primer hora)"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/tiempoBase"
            android:layout_below="@+id/informacion"
            android:textColor="#000000"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="$10.00"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/pb"
            android:layout_alignBaseline="@+id/tiempoBase"
            android:layout_alignParentRight="true"
            android:textColor="#000000"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tarifa (por hora o fraccion )"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/tiempoFrac"
            android:layout_below="@+id/tiempoBase"
            android:textColor="#000000"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="$5.00"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/pp"
            android:layout_alignBaseline="@+id/tiempoFrac"
            android:layout_alignParentRight="true"
            android:textColor="#000000"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Subtotal"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/sb"
            android:layout_below="@+id/sbTotal"
            android:textColor="#000000"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="$15.00"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/st"
            android:layout_alignBaseline="@+id/sb"
            android:layout_alignParentRight="true"
            android:textColor="#000000"
            />

        <TextView
            android:id="@+id/ds"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/sb"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:text="Estado"
            android:textColor="#000000"
            android:textSize="16sp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="$00.00"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/des"
            android:layout_alignBaseline="@+id/ds"
            android:layout_alignParentRight="true"
            android:textColor="#000000"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Total"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/tt"
            android:layout_below="@+id/ds"
            android:textColor="#000000"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="$15.00"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/to"
            android:layout_alignBaseline="@+id/tt"
            android:layout_alignParentRight="true"
            android:textColor="#000000"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pago"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/p"
            android:layout_below="@+id/tt"
            android:textColor="#000000"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="$15.00"
            android:textSize="16sp"
            android:layout_margin="10dp"
            android:padding="5dp"
            android:id="@+id/pa"
            android:layout_alignBaseline="@+id/p"
            android:layout_alignParentRight="true"
            android:textColor="#000000"
            />
    </RelativeLayout>
</android.support.constraint.ConstraintLayout>